<% if (success &&  success.length) { %> 
    <div class="alert alert-success" role="alert">
        <%= success %>
        <button type="button" class="close-btn" data-dismiss="alert" aria-label="Close">
            <span>&times</span>
        </button>
    </div>
<% } %> 

<% if (error &&  error.length) { %> 
    <div class="alert alert-error" role="alert">
        <%= error %>
        <button type="button" class="close-btn" data-dismiss="alert" aria-label="Close">
            <span>&times</span>
        </button>
    </div>
<% } %> 