<h1>My Works</h1>
<a href="/myworks/new">Add Story</a>
<% for (story of stories) { %>
    <div>
        <h2><%= story.title %></h2>
        <p><%= story.synopsis %></p>
        <a href="/myworks/<%= story.slug %>">Edit</a>
        <form action="/myworks/<%= story.slug%>?_method=DELETE" method="POST">
            <button type="submit">Delete</button>
        </form>
    </div>
<% } %> 